{
  "name": "abcaeffchen/sepa-utilities",
  "type": "library",
  "description": "SepaUtilities provides useful methods for validating and sanitizing inputs used in SEPA files supporting PHP >= 8.1.",
  "keywords": [
    "sepa",
    "validate",
    "sanitize",
    "iban",
    "bic",
    "php8"
  ],
  "homepage": "https://github.com/AbcAeffchen/SepaUtilities",
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Alexander Schickedanz (AbcAeffchen)",
      "email": "abcaeffchen@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/AbcAeffchen/SepaUtilities/issues"
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "ext-calendar": "*",
    "phpunit/phpunit": "^11 || ^10"
  },
  "autoload": {
    "psr-4": {
      "AbcAeffchen\\SepaUtilities\\": "src/"
    }
  }
}
